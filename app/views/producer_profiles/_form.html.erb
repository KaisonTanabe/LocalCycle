<%= form_for(@producer_profile) do |f| %>



    <div class="control-group">
      <%= f.label :phone, "Phone Number: ", class: "req control-label" %>
      <div class="controls controls-row">
	<%= f.text_field :phone, placeholder: "(123) 555-5555", class: "span4" %>
      </div>
    </div>

  <div class="field">
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
